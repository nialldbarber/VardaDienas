{
	"name": "VardaDienas",
	"version": "2.0.0",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"ios:sim": "npx react-native run-ios --simulator='iPhone 16 Pro'",
		"lint": "eslint .",
		"start": "react-native start",
		"test": "jest",
		"deps": "cd ios && npx pod install && cd ..",
		"version:patch": "./scripts/version.sh $(node -p \"require('./package.json').version.split('.').map((v,i)=>i===2?parseInt(v)+1:v).join('.')\")",
		"version:minor": "./scripts/version.sh $(node -p \"require('./package.json').version.split('.').map((v,i)=>i===1?parseInt(v)+1:i===2?0:v).join('.')\")",
		"version:major": "./scripts/version.sh $(node -p \"require('./package.json').version.split('.').map((v,i)=>i===0?parseInt(v)+1:i===1?0:i===2?0:v).join('.')\")",
		"version:set": "node -e \"if(!process.argv[1]){console.log('Usage: yarn version:set <version>'); process.exit(1)}; require('child_process').execSync('./scripts/version.sh ' + process.argv[1], {stdio: 'inherit'})\"",
		"version:commit": "git add . && git commit -m \"Bump version to $(node -p \"require('./package.json').version\")\"",
		"version:tag": "git tag v$(node -p \"require('./package.json').version\")",
		"version:push": "git push && git push --tags",
		"version:release": "yarn version:commit && yarn version:tag && yarn version:push"
	},
	"dependencies": {
		"@animatereactnative/accordion": "^0.1.5",
		"@gorhom/bottom-sheet": "^5",
		"@legendapp/list": "^1.0.14",
		"@legendapp/state": "^3.0.0-beta.30",
		"@notifee/react-native": "^9.1.8",
		"@react-navigation/bottom-tabs": "^7.3.14",
		"@react-navigation/drawer": "^7.4.1",
		"@react-navigation/native": "^7.1.10",
		"@react-navigation/native-stack": "^7.3.14",
		"@sentry/react-native": "^6.18.1",
		"@shopify/flash-list": "2.0.0-rc.2",
		"@shopify/react-native-skia": "^2.1.1",
		"@tanstack/react-form": "^1.12.0",
		"date-fns": "^4.1.0",
		"emoji-regex": "^10.4.0",
		"i18next": "^25.2.1",
		"iconsax-react-native": "^0.0.8",
		"react": "19.0.0",
		"react-i18next": "^15.5.2",
		"react-native": "0.79.2",
		"react-native-bootsplash": "^6.3.9",
